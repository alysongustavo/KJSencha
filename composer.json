{
    "name": "alysongustavo/kj-sencha",
    "description": "Ext-JS / Sencha Touch RPC Intergration Module for Mezzio Framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "lamminas",
        "sencha",
        "extjs"
    ],
    "homepage": "https://github.com/alysongustavo/KJSencha",
    "authors": [
        {
            "name": "Alyson Rodrigo",
            "email": "g.dasilva@hotmail.com",
            "homepage": "https://www.alysongustavoti.com.br"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laminas/laminas-mvc": "^3.1.1",
        "rwoverdijk/assetmanager": "^3.0.0",
        "ext-json": "*",
        "laminas/laminas-router": "^3.0.2",
        "laminas/laminas-eventmanager": "^3.4",
        "laminas/laminas-cache-storage-adapter-memory": "2.1.x-dev",
        "laminas/laminas-code": "3.4.x",
        "laminas/laminas-console": "2.9.x-dev",
        "doctrine/common": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "KJSencha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KJSenchaTest\\": "test/KJSenchaTest/",
            "KJSenchaTestAsset\\": "test/KJSenchaTestAsset/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    },
    "require-dev": {
        "laminas/laminas-test": "4.1.x-dev"
    }
}
